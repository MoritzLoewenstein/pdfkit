{
  "name": "@moritzloewenstein/pdfkit",
  "type": "module",
  "description": "A PDF generation library",
  "keywords": [
    "pdf",
    "pdf writer",
    "pdf generator",
    "graphics",
    "document",
    "vector"
  ],
  "version": "0.100.174",
  "homepage": "http://pdfkit.org/",
  "author": {
    "name": "Devon Govett",
    "email": "devongovett@gmail.com",
    "url": "http://badassjs.com/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MoritzLoewenstein/pdfkit.git"
  },
  "bugs": "https://github.com/MoritzLoewenstein/pdfkit/issues",
  "devDependencies": {
    "@biomejs/biome": "2.1.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "canvas": "^3.2.0",
    "codemirror": "~5.65.18",
    "jest-diff": "^30.1.2",
    "markdown": "^0.5.0",
    "pdfjs-dist": "^2.16.105",
    "pug": "^3.0.3",
    "vite": "^7.1.6",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "fontkit": "^2.0.4",
    "jpeg-exif": "^1.1.4",
    "linebreak": "^1.1.0",
    "md5-o-matic": "^0.1.1",
    "png-js": "^1.0.0",
    "uint8array-extras": "^1.5.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:watch": "vite build --watch",
    "biome": "biome check",
    "biome:write": "biome check --write",
    "biome:ci": "biome ci",
    "pdf-guide": "node docs/generate.js",
    "website": "node docs/generate_website.js",
    "publish-website": "node docs/publish_website.js",
    "docs": "npm run pdf-guide && npm run website",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:snapshot-update": "vitest -u",
    "test:coverage": "vitest run --coverage"
  },
  "engine": ["node >= v20.0.0"],
  "files": ["dist"],
  "main": "./dist/pdfkit.umd.cjs",
  "module": "./dist/pdfkit.js",
  "exports": {
    ".": {
      "import": "./dist/pdfkit.js",
      "require": "./dist/pdfkit.umd.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  },
  "volta": {
    "node": "22.19.0"
  }
}
